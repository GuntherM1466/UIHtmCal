            <setting name="ToolTipRegionCheck" serializeAs="String">
                <value>Enable this Region for input processing (Region is created fresh if parameters have changed since last on).</value>
            </setting>
            <setting name="ToolTipPerColText" serializeAs="String">
                <value>Percent of input bits within locality radius each Column has potential (proximal) synapses for.</value>
            </setting>
            <setting name="ToolTipLocalityRadText" serializeAs="String">
                <value>Furthest number of Columns away to allow distal synapse connections (0 means no restriction).</value>
            </setting>
            <setting name="ToolTipMinOverlapText" serializeAs="String">
                <value>Minimum percent of column's proximal synapses that must be active for the column to be considered by the spatial pooler.</value>
            </setting>
            <setting name="ToolTipDesireLocalText" serializeAs="String">
                <value>Approximate percent of Columns within locality radius to be winners after inhibition.</value>
            </setting>
