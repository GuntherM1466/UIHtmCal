                <value>Number of (temporal context) cells to use for each Column.</value>
            </setting>
            <setting name="ToolTipThresholdText" serializeAs="String">
                <value>Minimum number of active synapses to activate a segment.</value>
            </setting>
            <setting name="ToolTipNewSynapsesText" serializeAs="String">
                <value>Number of 'new' synapses to add to a segment if none activated during learning.</value>
            </setting>
            <setting name="ToolTipColXText" serializeAs="String">
